  <link rel="apple-touch-icon" sizes="120x120" href="old/img/fav/apple-icon-120x120.png">
  <link rel="apple-touch-icon" sizes="144x144" href="old/img/fav/apple-icon-144x144.png">
  <link rel="apple-touch-icon" sizes="152x152" href="old/img/fav/apple-icon-152x152.png">
  <link rel="apple-touch-icon" sizes="180x180" href="old/img/fav/apple-icon-180x180.png">
  <link rel="icon" type="image/png" sizes="192x192" href="old/img/fav/android-icon-192x192.png">
  <link rel="icon" type="image/png" sizes="32x32" href="old/img/fav/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="96x96" href="old/img/fav/favicon-96x96.png">
  <link rel="icon" type="image/png" sizes="16x16" href="old/img/fav/favicon-16x16.png">
